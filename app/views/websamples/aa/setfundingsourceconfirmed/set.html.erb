<html xmlns="http://www.w3.org/1999/xhtml" >
<head>
<title>Funding Source Details</title>
<%= stylesheet_link_tag "style"%>
</head>
<script type="text/javascript">
function PrePopulate()
{
	document.getElementById('email').value = "wchai-seller-usd1@paypal.com";
	
}
</script>

<body>
        <div id="jive-wrapper">
            <div id="jive-header">
                <div id="logo">
                    <span style="float: right; height: 30px;">Login to <a href="https://developer.paypal.com">PayPal sandbox</a></span>
                    <a title="Paypal X Home" href="#" id="web_pageheader_anchor_index"><%= image_tag 'paypal_x_logo.gif', :alt => 'Paypal X',:border=>'0', :height=>'28' %></a>
                </div>
            </div><h1>&nbsp;</h1>
        </div>
<%= render :partial => "layouts/menu" %>

<div id="request_form">

<center>
	<h3><b> &nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;&nbsp; Funding Source</b></h3>
	<a onclick="PrePopulate()"><span>Populate default values</span></a>
</center>
<br/>
  
<%=  form_tag(fundingSource_path, :method=>'post', :multipart => true, :id =>'AddPaymentCardDirectForm', :name => 'AddPaymentCardDirectForm') do %>
<table  align="center">

	<tr>
 		<td><label for="email">email Id:</label></td>
		<td><%= text_field_tag( "email", value = '',:size => 50, :maxlength => 80 ) %> </td>
	</tr>

	<tr>
 		<td><label for="fundingSourceKey">Funding Source Key:</label></td>
		<td><%= text_field_tag( "fundingSourceKey", value = '',:size => 50, :maxlength => 80 ) %> </td>
	</tr>
	</table>
<table align="center">
	<tr>
      	<td align="center">
<a class="pop-button primary" onclick="AddPaymentCardDirectForm.submit();" id="Submit"><span>Submit</span></a>
</td>
	</tr>

	
</table>
</div>
</body>
</html>
<%end%>