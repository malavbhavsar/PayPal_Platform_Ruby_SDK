<body>
        <div id="jive-wrapper">
            <div id="jive-header">
                <div id="logo">
                    <span style="float: right; height: 30px;">Login to <a href="https://developer.paypal.com">PayPal sandbox</a></span>
                    <a title="Paypal X Home" href="#" id="web_pageheader_anchor_index"><%= image_tag 'paypal_x_logo.gif', :alt => 'Paypal X',:border=>'0', :height=>'28' %></a>
                </div>
            </div><h1>&nbsp;</h1>
        </div>
<%= render :partial => "layouts/menu" %>

<div id="request_form">

<center>
	<h3><b> &nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp; Create Account Personal</b></h3>
	<a onclick="PrePopulate()"><span>Populate default values</span></a>
</center>
<br/>
<%=  form_tag(createaccount_path, :method=>'post', :multipart => true, :id => 'Form1', :name => 'Form1') do %>
<table  align="center">
	<tr>
 		<td>AccountType:</td>
		<td><%=account_select_for(:createaccount) %></td>
	</tr>
	<tr>
 		<td>Salutation:</td>
		<td><%=salutation_select_for(:createaccount) %></td>
	</tr>
	<tr>
 		<td><label for="firstname">First Name:</label></td>
		<td><%= text_field_tag( "firstname", value = '',:size => 50, :maxlength => 80 ) %> </td>
	</tr>
	<tr>
 		<td><label for="middlename">Middle Name:</label></td>
		<td><%= text_field_tag( "middlename", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="lastname">Last Name:</label></td>
		<td><%= text_field_tag( "lastname", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="dob">DOB:</label></td>
		<td><%= text_field_tag( "dob", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="address1">Address 1:</label></td>
		<td><%= text_field_tag( "address1", value = '',:size => 50, :maxlength => 80 ) %> </td>
	</tr>
	<tr>
 		<td><label for="address2">Address 2:</label></td>
		<td><%= text_field_tag( "address2", value = '',:size => 50, :maxlength => 80 ) %> </td>
	</tr>
	<tr>
 		<td><label for="city">City:</label></td>
		<td><%= text_field_tag( "city", value = '',:size => 50 , :maxlength => 80 ) %> </td>
	</tr>

	<tr>
 		<td>State:</td>
		<td><%=state_select_for(:createaccount) %></td>
	</tr>

	<tr>
 		<td><label for="zipcode">ZIP Code:</label></td>
		<td><%= text_field_tag( "zipcode", value = '',:size => 50, :maxlength => 80) %>(5 or 9 digits) </td>
	</tr>
	<tr>
 		<td><label for="countrycode">Country:</label></td>
		<td><%= text_field_tag( "countrycode", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="citzenshipcountry">CitizenshipCountryCode:</label></td>
		<td><%= text_field_tag( "citzenshipcountry", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="phone">Contact Phone Number:</label></td>
		<td><%= text_field_tag( "phone", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="url">Notification URL:</label></td>
		<td><%= text_field_tag( "url", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="p1">partnerField1:</label></td>
		<td><%= text_field_tag( "p1", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="p2">partnerField2:</label></td>
		<td><%= text_field_tag( "p2", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="p3">partnerField3:</label></td>
		<td><%= text_field_tag( "p3", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="p4">partnerField4:</label></td>
		<td><%= text_field_tag( "p4", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="p5">partnerField5:</label></td>
		<td><%= text_field_tag( "p5", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td>currencyCode:</td>
		<td><%=currency_select_for(:createaccount) %></td>
	</tr>
	<tr>
 		<td><label for="devCentral">Sandbox Developer Central <br>Email(Applicable <br> only for Sandbox):</label></td>
		<td><%= text_field_tag( "devCentral", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td style="width:300px"><label for="email">Email:</label></td>
		<td align="left" style="width: 160px"><%= text_field_tag( "email", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	</table>
<table align="center">
	<tr>
      	<td align="center">
<a class="pop-button primary" onclick="Form1.submit();" id="Submit"><span>Submit</span></a>
</td>
	</tr>
	
</table>
</div>
</body>
</html>
<%end%>

