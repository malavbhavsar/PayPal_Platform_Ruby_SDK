<html xmlns="http://www.w3.org/1999/xhtml" >
<head>
<title>Get Verified Status</title>
<%= stylesheet_link_tag "style"%>
</head>

<script type="text/javascript">
function PrePopulate()
{
document.getElementById('email').value = "platfo@paypal.com";
	document.getElementById('firstName').value = "Bonzop";
		document.getElementById('lastName').value = "Zaius";
document.getElementById('matchCriteria').value = "NAME";
}
</script>


<body>
        <div id="jive-wrapper">
            <div id="jive-header">
                <div id="logo">
                    <span style="float: right; height: 30px;">Login to <a href="https://developer.paypal.com">PayPal sandbox</a></span>
                    <a title="Paypal X Home" href="#" id="web_pageheader_anchor_index"><%= image_tag 'paypal_x_logo.gif', :alt => 'Paypal X',:border=>'0', :height=>'28' %></a>
                </div>
            </div><h1>&nbsp;</h1>
        </div>
<%= render :partial => "layouts/menu" %>

<div id="request_form">
<center>
	<h3><b> &nbsp;&nbsp;&nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;&nbsp; &nbsp;&nbsp; Get Verified Status</b></h3>
	<a onclick="PrePopulate()"><span>Populate default values</span></a>
</center>
<br/>
  
<%=  form_tag(verifiedStatus_path, :method=>'post', :multipart => true, :id => 'Form1', :name => 'Form1') do %>
<table  align="center">
	<tr>
 		<td><label for="email">Email Id:</label></td>
		<td><%= text_field_tag( "email", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>

	<tr>
 		<td><label for="firstName">First Name:</label></td>
		<td><%= text_field_tag( "firstName", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="lastName">Last Name:</label></td>
		<td><%= text_field_tag( "lastName", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	<tr>
 		<td><label for="matchCriteria">Match Criteria:</label></td>
		<td><%= text_field_tag( "matchCriteria", value = '',:size => 50, :maxlength => 80) %> </td>
	</tr>
	</table>
<table align="center">
	<tr>
      	<td align="center">
<a class="pop-button primary" onclick="Form1.submit();" id="Submit"><span>Submit</span></a>
</td>
	</tr>

</table>
</div>
</body>
</html>
<%end%>
